
red = "Red5"
orange = "Orange4"
yellow = "Yellow3"
green = "Green2"
blue = "Blue1"

def stackPush(stack):

    stack.append(red)
    stack.append(orange)
    stack.append(yellow)
    stack.append(green)
    stack.append(blue)
        
    return stack
    

def stackPop(stack):
    
    print("Pop :")
    
    for i in range(5):
        
        y = stack[-1]
        stack.pop()
        print(y)
        
    return stack
    
    
def removeTop(stack):
    
    element = stack[-1]
    print("Element on stack top :", element)
    stack.pop()
    
    

stack = []
stackPush(stack)
stackPop(stack)
stackPush(stack)




